{
  "name": "@hello-pangea/color-picker",
  "version": "2.19.4",
  "private": false,
  "description": "A Collection of Color Pickers from Sketch, Photoshop, Chrome & more",
  "main": "dist/color-picker.cjs.js",
  "module": "dist/color-picker.esm.js",
  "files": [
    "/dist",
    "/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-pangea/color-picker.git"
  },
  "author": "case <case@casesandberg.com>",
  "maintainers": [
    "Reece Carolan <reece@hellopangea.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hello-pangea/color-picker/issues"
  },
  "scripts": {
    "test": "npm run jest && npm run eslint",
    "jest": "jest",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "chromatic": "chromatic --project-token=9z2kaxsuz",
    "release": "release-it",
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "NODE_ENV=production rollup -c",
    "prepublishOnly": "npm run build"
  },
  "sideEffects": false,
  "keywords": [
    "react",
    "color picker",
    "react-component",
    "colorpicker",
    "picker",
    "sketch",
    "chrome",
    "photoshop",
    "material design",
    "popup"
  ],
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@icons/material": "^0.2.4",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "material-colors": "^1.2.1",
    "prop-types": "^15.5.10",
    "reactcss": "^1.2.0",
    "tinycolor2": "^1.4.1"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-react": "7.18.6",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@storybook/core": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "css-loader": "^0.24.0",
    "enzyme": "2.8.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "fbjs": "^0.8.6",
    "jest": "^20.0.4",
    "jsx-loader": "^0.13.2",
    "object-assign": "^4.1.0",
    "prettier": "^2.7.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^1.2.8",
    "react-test-renderer": "^15.3.2",
    "release-it": "^15.4.0",
    "remarkable": "^1.6.0",
    "rimraf": "^2.5.0",
    "rollup": "2.78.1",
    "rollup-plugin-dts": "4.2.2",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-terser": "7.0.2",
    "style-loader": "^0.13.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "rootDir": "src",
    "testRegex": "spec.js$"
  },
  "payload": {
    "builds": [
      {
        "cmd": "npm run docs-dist",
        "files": [
          "docs/build/bundle.js",
          "docs/index.js"
        ]
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
